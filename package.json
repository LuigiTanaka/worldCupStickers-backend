{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run prisma:deploy && node dist/src/server.js",
    "prisma:deploy": "prisma migrate deploy && prisma db seed",
    "dev": "dotenv -e .env.test nodemon src/server.ts",
    "prisma:test": "dotenv -e .env.test prisma migrate dev && dotenv -e .env.test prisma db seed",
    "test:integration": "npm run prisma-test && dotenv -e .env.test jest -- --no-cache --testPathPattern=tests/integration",
    "test:unit": "npm run prisma-test && dotenv -e .env.test jest -- -i --coverage --testPathPattern=tests/unit"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@prisma/client": "^4.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.1",
    "@types/supertest": "^2.0.12",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "prisma": "^4.4.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.2",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.20"
  }
}
